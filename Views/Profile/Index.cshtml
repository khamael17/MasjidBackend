                  </div> -->
                </div>
              </div>
        </div>
    

        <!-- Profile beginning -->
        <div class="col-lg-8 mx-0 px-0">
            <section id="profile" class="container px-0" style=" font-size: 15px;">

                <div class="card ">
                    <div class="card-body bg-color">
                        <div class="row ">
                            <div class="col d-flex justify-content-center">
                                <div class="prof-container">
                                    <div class="prof text-center">
                                        <img src="~/img/sheikh.jfif" alt="" class="img-thumbnail rounded-circle d-md-block" />
                                        <h7 class="imam-nav-name">Imam Khalilou</h7>

                                        <div>
                                            <a href="#" class="btn font-italic p-0 text-primary"
                                                style="font-size: 10px; ">
                                                <u>change picture <i class="fa fa-upload fa-1x"></i></u>

                                            </a>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <!-- Institution Name -->
                        <div class="row mx-4">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 px-0">

                                <form class=" px-3 mt-3">
                                    <div class="mb-3 text-center">
                                        <label for="InputFirstName" class="form-label"
                                            style="font-weight: 600;">Institution Name</label>
                                        <input type="text" class="form-control " id="inputFirstName"
                                            aria-describedby="emailHelp" style=" font-size: 8px;">
                                        <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
                                    </div>


                                    <button type="submit" class="btn btn-primary"
                                        style=" font-size: 10px;">Update</button>
                                </form>
                            </div>
                            <div class="col-lg-3"></div>



                            <!-- Institution Name -->
                            <!-- Update Name Beginning -->
                            <div class="row mx-4">
                                <div class="col-lg-3"></div>
                                <div class="col-lg-6 px-0">
                                    <div class="py-4 ">
                                        <h5><u>Name</u></h5>
                                    </div>
                                    <form class=" px-3 ">
                                        <div class="mb-3 ">
                                            <label for="InputFirstName" class="form-label">First Name</label>
                                            <input type="text" class="form-control " id="inputFirstName"
                                                aria-describedby="emailHelp" style=" font-size: 8px;">
                                            <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
                                        </div>
                                        <div class="mb-3">
                                            <label for="InputLastName" class="form-label">Last Name</label>
                                            <input type="text" class="form-control" id="inputLastName"
                                                style=" font-size: 8px;">
                                        </div>

                                        <button type="submit" class="btn btn-primary" style=" font-size: 10px;">Update
                                            Name</button>
                                    </form>
                                </div>
                                <div class="col-lg-3"></div>
                            </div>
                            <!-- Update Name Ending -->

                            <!-- Update Password Beginning -->
                            <div class="row mx-4">
                                <div class="col-lg-3  "></div>
                                <div class="col-lg-6 col-md-12 col-sm-12 px-0">
                                    <div class="py-4 ">
                                        <h5><u>Password</u></h5>
                                    </div>
                                    <form class=" px-3">
                                        <div class="mb-3 ">
                                            <label for="InputPassword" class="form-label">Old Password</label>
                                            <input type="password" class="form-control" id="inputPassword"
                                                aria-describedby="emailHelp" style=" font-size: 8px;">
                                        </div>
                                        <div class="">
                                            <label for="InputPassword" class="form-label">New Password</label>
                                            <input type="password" class="form-control" id="inputPassword"
                                                aria-describedby="emailHelp" style=" font-size: 8px;">
                                        </div>
                                        <div id="passwordHelpBlock" class="form-text mb-3 " style=" font-size:smaller">
                                            Your password must be 8-20 characters long, contain letters and numbers, and
                                            must not contain spaces, special characters, or emoji.
                                        </div>
                                        <div class="mb-3">
                                            <label for="InputConfirmPassword" class="form-label">Confirm
                                                Password</label>
                                            <input type="password" class="form-control" id="inputConfirmPassword"
                                                style=" font-size: 8px;">
                                        </div>

                                        <button type="submit" class="btn btn-primary" style=" font-size: 10px;">Update
                                            Password</button>
                                    </form>
                                </div>
                                <div class="col-lg-3"></div>

                            </div>
                            <!-- Update Password Ending -->


                            <!-- Prayer Time Beginning -->
                            <div id="prayer-time"class="row mx-4">
                                <div class="col-lg-3"></div>
                                <div class="col-lg-6 px-0">

                                    <div class="py-4 ">
                                        <h5><u>Prayer Times</u></h5>
                                        <form action="" class="">
                                            <div class="d-lg-inline-flex  ">
                                                <div class="mx-1 text-center itc">
                                                    <label for="fajr" class="form-label ">Fajr</label>
                                                    <input type="text" class="form-control" id="fajr"
                                                        aria-describedby="fajrPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>
                                                <div class="mx-1 text-center itc">
                                                    <label for="dhuhr" class="form-label">Dhuhr</label>
                                                    <input type="text" class="form-control" id="dhuhr"
                                                        aria-describedby="dhuhrPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>
                                                <div class="mx-1 text-center itc">
                                                    <label for="asr" class="form-label">Asr</label>
                                                    <input type="text" class="form-control" id="asr"
                                                        aria-describedby="asrPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>

                                            </div>
                                            <div class="d-flex mb-3">
                                                <div class="mx-1 text-center itc">
                                                    <label for="maghrib" class="form-label">Maghrib</label>
                                                    <input type="text" class="form-control" id="maghrib"
                                                        aria-describedby="maghribPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>
                                                <div class="mx-1 text-center itc">
                                                    <label for="isha" class="form-label">Isha</label>
                                                    <input type="text" class="form-control" id="isha"
                                                        aria-describedby="ishaPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>
                                                <div class="mx-1 text-center itc">
                                                    <label for="jummah" class="form-label">Jummah</label>
                                                    <input type="text" class="form-control" id="jummah"
                                                        aria-describedby="jummahPrayerTime"
                                                        style=" font-size: 10px; width:70px">
                                                </div>
                                            </div>
                                            <button type="submit" class="btn btn-primary"
                                                style=" font-size: 10px;">Update Prayer Times</button>
                                        </form>

                                    </div>
                                    <div class="col-lg-3"></div>
                                </div>
                                <!-- Prayer Time End -->
                            </div>
                        </div>
            </section>

            <!-- Profile end -->

            <!-- Feed beginning -->
            <section id="feed" class="container px-0">
                <div class="card">
                    <div class="card-body bg-color">
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <div class="py-4 ">
                                    <h5><u>Feed</u></h5>
                                </div>
                                <form class=" px-3">
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Feed</label>
                                        <textarea class="form-control" id="inputFeed" rows="8"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Narrator</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Who's posting?..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <fieldset id="kk" tag="kk" class="row mb-3">

                                        <div class="col-sm-10">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="gridRadios"
                                                    id="gridRadios1" value="option1" checked>
                                                <label class="form-check-label" for="gridRadios1">
                                                    General Announcement
                                                </label>
                                            </div>
                                            <div class="form-check">
                                                <input class="form-check-input bg-warning" type="radio"
                                                    name="gridRadios" id="gridRadios2" value="option2">
                                                <label class="form-check-label" for="gridRadios2">
                                                    Fund Raising
                                                </label>
                                            </div>
                                            <div class="form-check">
                                                <input class="form-check-input bg-danger" type="radio" name="gridRadios"
                                                    id="gridRadios2" value="option2">
                                                <label class="form-check-label" for="gridRadios2">
                                                    Funeral
                                                </label>
                                            </div>
                                            <div class="form-check">
                                                <input class="form-check-input bg-success" type="radio"
                                                    name="gridRadios" id="gridRadios2" value="option2">
                                                <label class="form-check-label" for="gridRadios2">
                                                    Request for Duaa
                                                </label>
                                            </div>

                                        </div>
                                    </fieldset>
                                    <button type="submit" class="btn btn-primary mb-4"style=" font-size: 10px;">Post</button>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </section>
            <!-- Feed end -->

            <!-- Event Beginning -->


            <section id="events" class="container px-0">
                <div class="card">
                    <div class="card-body bg-color">
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <div class="py-4 ">
                                    <h5><u>Events</u></h5>
                                </div>
                                <form class=" px-3">
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Event Title</label>
                                        <textarea class="form-control" id="inputFeed" rows="2"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Narrator</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Who's posting?..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label text-capitalize ">The Event</label>
                                        <textarea class="form-control" id="inputFeed" rows="6"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div>
                                        <a href="#" class="btn font-italic mb-3 text-primary" style="font-size: 10px; ">
                                            <u>Upload a picture <i class="fa fa-upload fa-1x"></i></u>

                                        </a>
                                    </div>
                                    <button type="submit" class="btn btn-primary mb-4" style=" font-size: 10px;">Post</button>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </section>
            <!-- Event end -->



            <!-- Islamic Classes Beginning -->
            <section id="ISclasse" class="container px-0">
                <div class="card">
                    <div class="card-body bg-color">
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <div class="py-4 ">
                                    <h5><u>Islamic Classes</u></h5>
                                </div>
                                <form class=" px-3">
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Class Title</label>
                                        <textarea class="form-control" id="inputFeed" rows="2"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Guest</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Who's lecturing?..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Time & Place</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Every Tuesday from 5:00pm to 6:00pm ..."
                                            style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label text-capitalize ">The Class</label>
                                        <textarea class="form-control" id="inputFeed" rows="6"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>

                                    <button type="submit" class="btn btn-primary mb-4"style=" font-size: 10px;">Post</button>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </section>



            <!-- Islamic Classes End -->


            <!-- Youth Program  Beginning-->
            <section id="Yprogram" class="container px-0">
                <div class="card">
                    <div class="card-body bg-color">
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <div class="py-4 ">
                                    <h5><u>Youth Program</u></h5>
                                </div>
                                <form class=" px-3">
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Program Title</label>
                                        <textarea class="form-control" id="inputFeed" rows="2"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Guest</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Who's lecturing?..." style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Time & Place</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Every Tuesday from 5:00pm to 6:00pm ..."
                                            style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 d-flex ">
                                        <label for="InputFeed" class="form-label itc mr-2">Price</label>
                                        <textarea class="form-control" id="inputFeed" rows="1" placeholder="$10..."
                                            style="font-size: 10px; width: 80px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label text-capitalize ">The Program</label>
                                        <textarea class="form-control" id="inputFeed" rows="6"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>

                                    <button type="submit" class="btn btn-primary mb-4" style=" font-size: 10px;">Post</button>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </section>




            <!-- Youth Program End -->

            <!-- After School and Day care Program Beginning -->


            <section id="A-Sprogram" class="container px-0">
                <div class="card">
                    <div class="card-body bg-color">
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <div class="py-4 ">
                                    <h5><u>After School and Day Care Program</u></h5>
                                </div>
                                <form class=" px-3">
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label">Program Title</label>
                                        <textarea class="form-control" id="inputFeed" rows="2"
                                            placeholder="Day Care..karate clas..." style="font-size: 10px;"></textarea>
                                    </div>

                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label ">Time & Place</label>
                                        <textarea class="form-control" id="inputFeed" rows="1"
                                            placeholder="Every Tuesday from 5:00pm to 6:00pm ..."
                                            style="font-size: 10px;"></textarea>
                                    </div>
                                    <div class="mb-3 d-flex ">
                                        <label for="InputFeed" class="form-label itc mr-2">Price</label>
                                        <textarea class="form-control" id="inputFeed" rows="1" placeholder="$10..."
                                            style="font-size: 10px; width: 80px;"></textarea>
                                    </div>
                                    <div class="mb-3 ">
                                        <label for="InputFeed" class="form-label text-capitalize ">The Program</label>
                                        <textarea class="form-control" id="inputFeed" rows="6"
                                            placeholder="Type something..." style="font-size: 10px;"></textarea>
                                    </div>

                                    <button type="submit" class="btn btn-primary mb-4" style=" font-size: 10px;">Post</button>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </section>



            <!-- After School Day Care Program End -->

        </div>

    </div>


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <script src="./js/jquery-3.6.3.min.js"></script>
    <script src=".js/bootstrap.bundle.min.js"></script>
</body>

</html>
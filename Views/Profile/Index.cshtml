        crossorigin="anonymous"></script>

    <script defer src="./js/brands.js"></script>
    <script defer src="./js/solid.js"></script>
    <script defer src="./js/fontawesome.js"></script>
</head>

<body>



    <div class="profile-menu d-flex ">



        
        <div class=" col-lg-3  mx-0 c0149 " style="" >
            <div class="menu  p-0" style="max-height: 80vh">
                <div class="card bg-color" style="max-height: 80vh">
                  <div
                    class="card-header bg-primary text-white text-uppercase text-center py-3"
                  >
                    <h5>Settings</h5>
                  </div>
        
                  <ul class="list-group list-group-flush text-center">
                    <li class="list-group-item" style="background-color: aliceblue">
                        <a href="#profile" class="btn font-italic"><u>Profile</u></a>
                      </li>
                      <li class="list-group-item" style="background-color: aliceblue">
                        <a href="#prayer-time" class="btn font-italic"><u>Prayer Time</u></a>
                      </li>
                      <li class="list-group-item" style="background-color: aliceblue">
                        <a href="#feed" class="btn font-italic"><u>Feed</u></a>
                      </li>
                      <li class="list-group-item" style="background-color: aliceblue">
                        <a href="#events" class="btn font-italic"><u>Events</u></a>
                      </li>
                    <li class="list-group-item" style="background-color: aliceblue">
                      <a href="#Yprogram" class="btn font-italic"><u>Youth Program</u></a>
                    </li>
                    <li class="list-group-item" style="background-color: aliceblue">
                      <a href="#Isclass" class="btn font-italic"><u>Islamic Classes</u></a>
                    </li>
                    
                   
                    <li class="list-group-item" style="background-color: aliceblue">
                        <a href="#A-Sprogram" class="btn font-italic">
                          <u>After-School Program</u></a
                        >
                      </li>
                    
                  </ul>
